   "source": [
    "## Formula\n",
    "\n",
    "# $f'(x) = \\frac{f(x_{i+1}) - f(x_i)}{h}$"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = [1, 3, 4, 5, 6]\n",
    "def derivada(x1, x2, h):\n",
    "    return (x1 - x2) / h\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
